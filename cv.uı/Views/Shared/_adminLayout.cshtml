<!DOCTYPE html>
<html lang="en">

@await Html.PartialAsync("~/views/adminLayout/head.cshtml")

<body id="page-top">

    <!-- Page Wrapper -->
    <div id="wrapper">

        <!-- Sidebar -->
        @await Html.PartialAsync("~/views/adminLayout/navbar.cshtml")
        <div id="content-wrapper" class="d-flex flex-column">

            <!-- Main Content -->
            <div id="content">
                <!-- Begin Page Content -->
                <div class="container-fluid">
                    <!-- Content Row -->
                    <div class="row">
                        <div class="col-lg-12 mb-4 my-3">

                            @RenderBody()
                        </div>
                    </div>

                </div>
            </div>
        </div>
    </div>

    <a class="scroll-to-top rounded" href="#page-top">
        <i class="fas fa-angle-up"></i>
    </a>


    @await Html.PartialAsync("~/views/adminLayout/scripts.cshtml")

</body>

</html>